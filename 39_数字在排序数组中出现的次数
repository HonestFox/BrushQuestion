#include<iostream>
#include<vector>
using namespace std;

class Solution
{
public:
//普通的方法
	int GetNumberOfK(vector<int> data, int k) 
	{
		if (data.empty())
		{
			return 0;
		}
		int RetCount = 0;
		for (auto &i : data)
		{
			if (i == k)
			{
				++RetCount;
			}
			if (RetCount != 0 && i != k)
			{
				break;
			}
		}
		return RetCount;
	}
};

void Test()
{
	vector<int> ivec1 = { 1,  3, 5, 7, 7, 8 };
	cout << Solution().GetNumberOfK(ivec1, 7) << endl;
}
int main()
{
	Test();
	return 0;
}
